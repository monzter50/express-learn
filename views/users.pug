//- sub-layout.pug
extends main-layout/layout.pug

block content
    main
        if usersData.length > 0
            .grid
                each user in usersData
                    div.card
                        div.card-image
                            img(src="data:image/png;base64," alt="") 
                            span.card-title #{user.username}
                            small.card-subtitle #{user.lastname}
                        div.card-content
                            p #{user.description}
                        div.card-action
                            a(href="#") This is a link
                    
                        
        else
            h1 No Products